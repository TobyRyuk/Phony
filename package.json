    "scripts": {
        "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
        "prepare": "node config && webpack && cordova prepare",
        "build": "node config && webpack && cordova build",
        "cleanse": "rimraf platforms/*",
        "android": "yarn run prepare && cordova platform add android ; cordova run android",
        "ios": "yarn run prepare && cordova platform add ios ; cordova run ios",
        "lint": "eslint --ext .js,.vue src",
        "serve": "phonegap serve --port 8888",
        "webpacker": "node config && webpack --progress --colors --watch",
        "development": "npm-run-all --parallel webpacker serve",
        "test": "echo \"No tests (yet!) -- submit a PR?\" && exit 0"
